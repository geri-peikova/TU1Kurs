#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<stdlib.h>
#include <stdbool.h>
#include<Windows.h>

int main() {
	int c;
	FILE *file;
	file = fopen("C:\\Users\\Student\\source\\repos\\Project2\\Project2\\Debug\\b.txt.txt", "r");
	if (file) {
		while ((c = getc(file)) != EOF)
			putchar(c);
		fclose(file);
	}

	system("pause");

	return 0;

}
